    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "css-loader": "0.28.7",
    "dotenv-webpack": "1.5.4",
    "extract-text-webpack-plugin": "3.0.2",
    "mocha": "4.0.1",
    "nodemon": "1.12.1",
    "nyc": "11.3.0",
    "sinon": "4.1.1",
    "snazzy": "7.0.0",
