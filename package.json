  "repository": {
    "type": "git",
    "url": "git+https://github.com/gperreymond/vue-nova-cms.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gperreymond/vue-nova-cms/issues"
  },
  "homepage": "https://github.com/gperreymond/vue-nova-cms#readme",
  "dependencies": {
    "bell": "8.8.0",
    "bluebird": "3.5.1",
    "catbox": "9.0.0",
    "catbox-memcached": "1.1.1",
    "catbox-memory": "3.0.0",
    "debug": "3.1.0",
    "glob-promise": "3.3.0",
    "hapi": "16.6.2",
    "hapi-auth-jwt2": "7.3.0",
    "inert": "4.2.1",
    "nconf": "0.9.1",
    "vision": "4.1.0",
    "vue": "2.5.3",
    "vue-router": "3.0.1",
    "yamljs": "0.3.0"
